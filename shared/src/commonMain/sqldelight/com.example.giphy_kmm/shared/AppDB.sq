CREATE TABLE ScrapTable (
id TEXT NOT NULL PRIMARY KEY,
title TEXT NOT NULL,
url TEXT NOT NULL,
downsizedUrl TEXT NOT NULL
);

loadScrapLists:
SELECT *
FROM ScrapTable;

insertScrapTable:
INSERT INTO ScrapTable (id, title, url, downsizedUrl)
VALUES(?, ?,?, ?);

deleteScrapTable:
DELETE FROM ScrapTable
WHERE id = :selectedId;

